.activity-schedule.invited-guest
  .table-responsive
    table.table.table-dark.table-striped
      thead
        tr
          th 邀请 Runner
          th 游戏
          th 规则
          th 预计时间
      tbody
        - invited_guest.forEach(x => {
          tr
            != partial('_table/td-runner', { _runner: x._runner })
            != partial('_table/td-game', { _game: x._game })
            td
              = x._rule
            td
              = x._est
        - })